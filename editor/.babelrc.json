{
    "presets": [
        "@babel/preset-react",
        "@babel/preset-typescript"
    ],
    "plugins": [
        "@babel/proposal-class-properties",
        "@babel/proposal-object-rest-spread"
    ],
    "targets": [
        "last 2 chrome versions",
        "last 2 firefox versions",
        "last 2 safari versions"
    ]
}